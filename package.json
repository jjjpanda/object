{
  "name": "object",
  "version": "2.0.0",
  "description": "object detection and webhook alerts using tensorflow-js coco-ssd",
  "main": "./lib/index.js",
  "bin": {
    "object": "./bin/object.js"
  },
  "engines": {
    "npm": "^6.14.4 || ^8.2.0",
    "node": ">=10.0.0 <15.0.0"
  },
  "scripts": {
    "prebuild-linux": "npm install --development",
    "build-linux": "npx pkg . --compress Brotli -t node12-linux -o ./dist/object-linux",
    "postbuild-linux": "npm i --production --package-lock-only && npm shrinkwrap",
    "prebuild-win": "npm install --development",
    "build-win": "npx pkg . --compress Brotli -t node12-win -o ./dist/object-win.exe",
    "postbuild-win": "npm i --production --package-lock-only && npm shrinkwrap"
  },
  "pkg": {
    "assets": [
      "node_modules/@jimp/plugin-print/fonts/open-sans/open-sans-16-white/open-sans-16-white.fnt",
      "node_modules/@jimp/plugin-print/fonts/open-sans/open-sans-16-white/open-sans-16-white.png",
      "node_modules/@tensorflow/tfjs-node/lib/napi-v8/tensorflow.dll",
      "node_modules/@tensorflow/tfjs-node/lib/napi-v8/tfjs_binding.node"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jjjpanda/object.git"
  },
  "author": "jjjpanda",
  "license": "MIT",
  "devDependencies": {
    "@tensorflow-models/coco-ssd": "^2.2.2",
    "@tensorflow/tfjs-node": "3.11.0",
    "axios": "^0.24.0",
    "dotenv": "^10.0.0",
    "form-data": "^4.0.0",
    "jimp": "^0.16.1",
    "pkg": "^5.5.1"
  }
}
