{
  "name": "object",
  "version": "2.0.2",
  "description": "object detection and webhook alerts using tensorflow-js coco-ssd",
  "main": "./lib/index.js",
  "bin": {
    "object": "./bin/object.js"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.0.0 <17.0.0"
  },
  "scripts": {
    "prebuild": "npm install && npm rebuild @tensorflow/tfjs-node --build-from-source",
    "build": "pkg . --compress Brotli",
    "postbuild": "npm i --no-optional --package-lock-only && npm shrinkwrap",
    "build-linux": "npm run build -- -t node16-linux-x64 -o ./dist/object-linux",
    "build-win": "npm run build -- -t node16-win-x64 -o ./dist/object-win.exe",
    "webpackCont": "webpack --progress --watch",
    "prestart": "webpack --progress",
    "start": "node live.js"
  },
  "pkg": {
    "assets": [
      "./node_modules/@jimp/plugin-print/fonts/open-sans/open-sans-16-white/open-sans-16-white.fnt",
      "./node_modules/@jimp/plugin-print/fonts/open-sans/open-sans-16-white/open-sans-16-white.png",
      "./node_modules/@tensorflow/tfjs-node/lib/napi-v8/**"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jjjpanda/object.git"
  },
  "author": "jjjpanda",
  "license": "MIT",
  "optionalDependencies": {
    "@tensorflow/tfjs-node": "3.11.0",
    "axios": "^0.24.0",
    "dotenv": "^10.0.0",
    "form-data": "^4.0.0",
    "jimp": "^0.16.1",
    "pkg": "^5.5.1"
  },
  "dependencies": {
    "@babel/core": "^7.16.7",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-transform-runtime": "^7.16.7",
    "@babel/preset-env": "^7.16.7",
    "@babel/preset-react": "^7.16.7",
    "@tensorflow-models/coco-ssd": "^2.2.2",
    "@tensorflow/tfjs": "^3.12.0",
    "babel-loader": "^8.2.3",
    "babel-plugin-import": "^1.13.3",
    "css-loader": "^6.5.1",
    "dotenv-webpack": "^7.0.3",
    "express": "^4.17.2",
    "html-webpack-plugin": "^5.5.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-hls-player": "^3.0.7",
    "style-loader": "^3.3.1",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1"
  }
}
