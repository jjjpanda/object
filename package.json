{
  "name": "object",
  "version": "1.1.2",
  "description": "object detection and webhook alerts using tensorflow-js coco-ssd",
  "main": "./lib/index.js",
  "bin": {
    "object": "./bin/object.js"
  },
  "engines": {
    "npm": "^6.14.4 || ^8.2.0",
    "node": ">=12.0.0 <15.0.0"
  },
  "scripts": {
    "prebuild": "npm install --development",
    "build": "npx pkg . --compress Brotli",
    "postbuild": "npm i --production --package-lock-only && npm shrinkwrap"
  },
  "pkg": {
    "assets": [
      "node_modules/@jimp/plugin-print/fonts/open-sans/open-sans-16-white/open-sans-16-white.fnt",
      "node_modules/@jimp/plugin-print/fonts/open-sans/open-sans-16-white/open-sans-16-white.png",
      "node_modules/@tensorflow/tfjs-node/lib/napi-v8/tensorflow.dll",
      "node_modules/@tensorflow/tfjs-node/lib/napi-v8/tfjs_binding.node"
    ],
    "targets": [
      "node12-linux",
      "node12-win"
    ],
    "outputPath": "dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jjjpanda/object.git"
  },
  "author": "jjjpanda",
  "license": "MIT",
  "devDependencies": {
    "@tensorflow-models/coco-ssd": "^2.2.2",
    "@tensorflow/tfjs-node": "^3.11.0",
    "axios": "^0.24.0",
    "form-data": "^4.0.0",
    "jimp": "^0.16.1",
    "pkg": "^5.5.1"
  },
  "dependencies": {
    "dotenv": "^10.0.0"
  }
}
