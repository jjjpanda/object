{
  "name": "object",
  "version": "1.1.1",
  "description": "object detection and webhook alerts using tensorflow-js coco-ssd",
  "main": "./lib/index.js",
  "bin": {
    "object": "./bin/object.js"
  },
  "scripts": {
    "prebuild": "npm install --include=dev",
    "build": "npx pkg . -t node12-linux,node12-macos,node12-win -o dist/object",
    "postbuild": "npm prune --production && npm i --production --package-lock-only && npm shrinkwrap"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jjjpanda/object.git"
  },
  "author": "jjjpanda",
  "license": "MIT",
  "devDependencies": {
    "@tensorflow-models/coco-ssd": "^2.2.2",
    "@tensorflow/tfjs-node": "^3.11.0",
    "axios": "^0.24.0",
    "form-data": "^4.0.0",
    "jimp": "^0.16.1",
    "pkg": "^5.5.1"
  }
}
